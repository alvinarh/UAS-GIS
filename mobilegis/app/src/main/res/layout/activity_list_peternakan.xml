<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListPeternakan"

    >



    <ImageButton
        android:id="@+id/btnback2"
        android:layout_width="20dp"
        android:layout_height="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
         />

    <TextView
        android:id="@+id/tvinputdata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnback2"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"

        android:text="Daftar Data"
        android:textColor="@color/black"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/tvpeternakan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvinputdata"
        android:layout_marginLeft="20dp"

        android:text="Hotel berbintang dan non berbintang"
        android:textColor="@color/black"
        android:textSize="15sp" />



    <TextView
        android:id="@+id/tvlistData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvpeternakan"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="50dp"

        android:text="List Data"
        android:textColor="@color/black"
        android:textSize="20sp" />


    <ListView

        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tvlistData"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="200dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="3dp" />

</RelativeLayout>